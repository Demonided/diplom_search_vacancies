<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/fragment_default_title_height"
        app:title="@string/bottomTeam"
        app:titleTextAppearance="@style/medium22_font"/>

    <TextView
        style="@style/bold32_font"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin"
        android:layout_marginTop="@dimen/margin"
        android:text="@string/developers"
        android:textColor="@color/black" />

    <TextView
        style="@style/medium16_font"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin"
        android:layout_marginTop="@dimen/l_margin"
        android:text="@string/developer_maksim"
        android:textColor="@color/black" />

    <TextView
        style="@style/medium16_font"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin"
        android:layout_marginTop="@dimen/margin"
        android:text="@string/developer_sergey"
        android:textColor="@color/black" />

    <TextView
        style="@style/medium16_font"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin"
        android:layout_marginTop="@dimen/margin"
        android:text="@string/developer_anton"
        android:textColor="@color/black" />

    <TextView
        style="@style/medium16_font"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin"
        android:layout_marginTop="@dimen/margin"
        android:text="@string/developer_dmitry"
        android:textColor="@color/black" />

    <TextView
        style="@style/medium16_font"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin"
        android:layout_marginTop="@dimen/margin"
        android:text="@string/developer_igor"
        android:textColor="@color/black" />

</LinearLayout>
